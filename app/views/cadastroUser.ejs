                                        </select>
                                        <div class="select-dropdown"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row row-space">
                            <div class="col-2">
                                <div class="input-group">
                                    <div class="rs-select2 js-select-simple select">
                                        <select name="department" id="setor" onchange="setorMudanca(this)" required>
                                            <option disabled="disabled" selected="selected">Setor</option>
